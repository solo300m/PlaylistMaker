<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sharing.ui.activity.MediatekaActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="@dimen/size_24"
        >
        <ImageView
            android:id="@+id/backMediaToMain"
            android:layout_width="@dimen/button_48_size"
            android:layout_height="@dimen/button_48_size"
            android:backgroundTint="@color/white"
            android:src="@drawable/left"
            android:layout_gravity="center"
            android:gravity="center"
            android:padding ="@dimen/size_12"

            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/mediateka"
            android:textSize="@dimen/app_font_size"
            android:textStyle="bold"
            android:paddingBottom="@dimen/size_12"
            android:paddingTop="@dimen/size_12"
            android:paddingStart="@dimen/size_12"
            android:paddingEnd="@dimen/size_86"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
